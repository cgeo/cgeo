    <groundspeak:cache id="2877444" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Flughafenrunde (Urdenbach zum 1000.)</groundspeak:name>
      <groundspeak:placed_by>wogami</groundspeak:placed_by>
      <groundspeak:owner id="3517008">wogami</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
      <groundspeak:difficulty>2</groundspeak:difficulty>
