<script type="text/javascript">
    $("a.CoordInfoLink").click(function (e) {
        e.preventDefault();

        $("#dlgClipboard")
                    .show()
                    .position({
                        of: $("a.CoordInfoLink"),
                        my: "right top",
                        at: "right bottom",
                        offset: "0 5"
                    })
                    .find("input")
                        .val('http://coord.info/' + $('.CoordInfoCode').text())
                        .focus()
                        .select();

    });

    $(document).mouseup(function (e) {
        if ($(e.target).parent("div#dlgClipboard").length == 0) {
            $("div#dlgClipboard").hide();
        }
    });
</script>

